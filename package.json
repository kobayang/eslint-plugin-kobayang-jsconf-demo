{
  "name": "eslint-plugin-kobayang-jsconf-demo",
  "version": "1.0.1",
  "main": "lib/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.2.0",
    "eslint": "^8.3.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testEnvironment": "node"
  }
}
